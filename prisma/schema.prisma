generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id        Int     @id @default(autoincrement())
  title     String
  body      String
  published Boolean
}

model Product {
  id    Int    @id @default(autoincrement())
  name  String
  price Float
  brand String
}
